{
  "manifest_version": 2,
  "name": "Be Well-ness",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["p5.min.js", "script.js"]
    }],
    "background":{
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "well.png",
      "default_popup": "popup.html",
      "default_title": "Be Well-ness Options"
    }

}
